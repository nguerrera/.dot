[include]
	path = shared.gitconfig
[credential]
	helper = /mnt/c/Program\\ Files/Git/mingw64/bin/git-credential-manager.exe
[difftool "bc"]
	path = bcomp-wsl
[alias]
	dd = difftool --no-symlinks --dir-diff
	dt = difftool --no-symlinks -y
[url "__DO_NOT_PUSH__/"]
	pushinsteadof = https://github.com/
[url "https://github.com/nguerrera/"]
	pushinsteadof = https://github.com/nguerrera/
[credential "dev.azure.com"]
	useHttpPath = true
[credential "visualstudio.com"]
	useHttpPath = true
